{
  "expo": {
    "name": "deutsch",
    "slug": "deutsch",
    "version": "1.0.0",
    "runtimeVersion": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pretzel.jpeg",
    "scheme": "deutsch",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "CFBundleLocalizations": ["fr"]
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.duderd.deutsch"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.duderd.deutsch"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
  "expo-router",
  [
    "@stripe/stripe-react-native",
    {
      "merchantIdentifier": "merchant.com.duderd.deutsch",
      "enableGooglePay": true
    }
  ],
  [
    "expo-splash-screen",
    {
      "image": "./assets/images/splash-icon.png",
      "imageWidth": 200,
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    }
  ]
]
  },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4d9c997f-01be-4c16-9c50-a39bc0f78c67"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/4d9c997f-01be-4c16-9c50-a39bc0f78c67"
    }
  }
